{
  "name": "radiant",
  "version": "0.1.0",
  "private": true,
  "config": {
    "dev": {
      "app": {
        "host": "localhost",
        "port": "3000"
      },
      "api": {
        "port": "3001"
      }
    }
  },
  "scripts": {
    "env-vars": "set | sort",
    "start": ": ${REACT_APP_API_BASE_URL:=${npm_package_config_default_api_baseUrl}}; export REACT_APP_API_BASE_URL; npm run start-app",
    "start-app": ": ${PORT:=${npm_package_config_dev_app_port}}; node scripts/start.js",
    "start-mock": "concurrently --kill-others \"npm run start-mock-api\" \"REACT_APP_API_BASE_URL=http://localhost:${npm_package_config_dev_api_port} npm run start\"",
    "build": ": ${REACT_APP_API_BASE_URL:=${npm_package_config_default_api_baseUrl}}; export REACT_APP_API_BASE_URL; node scripts/build.js",
    "start-mock-api": ": ${MOCK_API_DATA_DIR:=$(npm root)/../data}; serve --cors -p ${npm_package_config_dev_api_port} ${MOCK_API_DATA_DIR}",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --ext js,jsx,ts,tsx $(npm root)/../src/",
    "stylelint": "stylelint $(npm root)/../src/",
    "test-coverage": "npm run test -- --coverage --all",
    "visualiser": "VISUALIZER=true npm run build && xdg-open $(npm root)/../build/webpack-visualizer-stats.html",
    "visualizer": "npm run visualiser",
    "generate": "plop"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/fs-extra": "^5.0.1",
    "@types/jest": "^22.2.2",
    "@types/lodash": "^4.14.108",
    "@types/moment-timezone": "^0.5.4",
    "@types/nock": "^9.1.2",
    "@types/react": "^16.3.17",
    "@types/react-addons-css-transition-group": "^15.0.4",
    "@types/react-dom": "^16.0.6",
    "@types/react-test-renderer": "^16.0.1",
    "@types/webpack-env": "^1.13.5",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.15.0-rc.8",
    "jest-styled-components": "^5.0.1",
    "nock": "^9.2.3",
    "plop": "^2.0.0",
    "react-test-renderer": "^16.2.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "typescript-eslint-parser": "^14.0.0"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.48",
    "@babel/helpers": "7.0.0-beta.48",
    "@babel/parser": "7.0.0-beta.48",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.48",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.48",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.48",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.48",
    "@babel/plugin-transform-runtime": "7.0.0-beta.48",
    "@babel/polyfill": "7.0.0-beta.48",
    "@babel/preset-env": "7.0.0-beta.48",
    "@babel/preset-typescript": "7.0.0-beta.48",
    "@babel/runtime": "7.0.0-beta.48",
    "autoprefixer": "7.2.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-named-asset-import": "0.0.0",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-app": "4.0.0-next.47d2d941",
    "camelcase-keys-deep": "^0.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "concurrently": "^3.5.1",
    "css-loader": "0.28.9",
    "dotenv": "5.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-typescript": "^0.9.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "fs-extra": "5.0.0",
    "graphql": "0.12.3",
    "graphql-tag": "2.6.1",
    "hard-source-webpack-plugin": "^0.6.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "intersection-observer": "^0.5.0",
    "jest": "^22.4.2",
    "lodash": "^4.17.10",
    "markdown-loader": "^2.0.2",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "mobx-react-devtools": "^5.0.1",
    "mobx-state-tree": "^2.0.3",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "mst-middlewares": "^2.0.3",
    "node-object-hash": "^1.3.0",
    "object-assign": "4.1.1",
    "polished": "^1.9.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.10",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.0",
    "react-autocomplete": "^1.8.1",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.4.0",
    "react-transition-group": "^2.3.1",
    "remotedev": "^0.2.7",
    "serve": "^6.5.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "0.19.1",
    "styled-animate": "^0.2.0",
    "styled-components": "^3.1.6",
    "svgr": "1.8.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "thread-loader": "1.1.2",
    "transformation-matrix": "^1.11.0",
    "ts-loader": "^3.5.0",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "1.1.6",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.0",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "whatwg-fetch": "2.0.3"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions",
      "not ie < 11"
    ],
    "production": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions",
      "not ie < 11"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/config.ts",
      "!src/index.tsx",
      "!src/helpers/gtm.ts",
      "!src/registerServiceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 25,
        "functions": 25,
        "lines": 25,
        "statements": 25
      }
    },
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true
      }
    },
    "setupFiles": [
      "<rootDir>/config/enzymeSetup.js",
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "babel-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "modulePathIgnorePatterns": [
      "node_modules",
      "config",
      "public"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ]
  }
}
